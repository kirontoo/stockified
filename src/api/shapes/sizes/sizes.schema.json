{
	"$schema": "http://json-schema.org/draft-07/schema",
	"type": "object",
	"title": "Item",
	"description": "The Item",
	"required": [
		"name",
		"shape_id",
		"measurement_unit_id"
	],
	"additionalProperties": false,
	"properties": {
		"id": {
			"$id": "#/properties/id",
			"type": "integer",
			"title": "The database ID of the size."
		},
		"name": {
			"$id": "#/properties/name",
			"type": "string",
			"title": "The name of the size."
		},
		"length": {
			"$id": "#/properties/length",
			"type": "number",
			"title": "The length of the size."
		},
		"width": {
			"$id": "#/properties/width",
			"type": "number",
			"title": "The width of the size."
		},
		"height": {
			"$id": "#/properties/height",
			"type": "number",
			"title": "The height of the size."
		},
		"volume": {
			"$id": "#/properties/volume",
			"type": "number",
			"title": "The volume of the size."
		},
		"measurement_unit_id": {
			"$id": "#/properties/measurement_unit_id",
			"type": "integer",
			"title": "The id number of the unit of measurement"
		},
		"shape_id": {
			"$id": "#/properties/shape_id",
			"type": "integer",
			"title": "The id number of a shape"
		},
		"deleted_at": {
			"$id": "#/properties/deleted_at",
			"type": "string",
			"title": "The deleted at date"
		}
	}
}
