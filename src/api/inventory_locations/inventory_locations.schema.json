{
	"$schema": "http://json-schema.org/draft-07/schema",
	"type": "object",
	"title": "Inventory Location",
	"description": "The Inventory Location",
	"required": [
		"name"
	],
	"additionalProperties": false,
	"properties": {
		"id": {
			"$id": "#/properties/id",
			"type": "integer",
			"title": "The database ID of the inventory location."
		},
		"name": {
			"$id": "#/properties/name",
			"type": "string",
			"title": "The name of the inventory location."
		},
		"description": {
			"$id": "#/properties/description",
			"type": "string",
			"title": "The description of the inventory location."
		},
		"image_url": {
			"$id": "#/properties/image_url",
			"type": "string",
			"title": "The image url of the inventory location"
		},
		"deleted_at": {
			"$id": "#/properties/deleted_at",
			"type": "string",
			"title": "The deleted at date."
		}
	}
}
