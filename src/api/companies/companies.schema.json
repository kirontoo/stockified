{
	"$schema": "http://json-schema.org/draft-07/schema",
	"type": "object",
	"title": "Company",
	"description": "A Company",
	"required": [
		"name",
		"address_id"
	],
	"additionalProperties": false,
	"properties": {
		"id": {
			"$id": "#/properties/id",
			"type": "integer",
			"title": "The database ID of the Company."
		},
		"name": {
			"$id": "#/properties/name",
			"type": "string",
			"title": "The name of the Company."
		},
		"description": {
			"$id": "#/properties/description",
			"type": "string",
			"title": "The description of the Company."
		},
		"logo_url": {
			"$id": "#/properties/logo_url",
			"type": "string",
			"title": "The id of the Company type."
		},
		"website_url": {
			"$id": "#/properties/website_url",
			"type": "string",
			"title": "The website url of the company."
		},
		"email": {
			"$id": "#/properties/email",
			"type": "string",
			"title": "The email of the company."
		},
		"address_id": {
			"$id": "#/properties/address_id",
			"type": "integer",
			"title": "The id of the company address."
		},
		"deleted_at": {
			"$id": "#/properties/deleted_at",
			"type": "string",
			"title": "The deleted at date of the company."
		}
	}
}
