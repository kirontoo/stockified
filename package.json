    "compression": "^1.7.4",
    "express": "^4.17.1",
    "helmet": "^4.1.1",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.21.6",
    "morgan": "^1.10.0",
    "objection": "^2.2.3",
    "papaparse": "^5.3.0",
    "pg": "^8.4.1",
