{
  "name": "stockified-api",
  "version": "1.0.0",
  "description": "An api for a home inventory database",
  "main": "index.js",
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "migrate": "knex migrate:latest",
    "rollback": "knex migrate:rollback",
